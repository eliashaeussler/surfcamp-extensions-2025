{
	"name": "TYPO3 SurfCamp 2025",
	"dockerComposeFile": "docker-compose.yml",
	"service": "typo3",
	"workspaceFolder": "/workspace",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-azuretools.vscode-docker"
			]
		}
	},
	"remoteUser": "vscode",
	"postCreateCommand": "bash .github/.devcontainer/post-start.sh",
	"postStartCommand": "./bin/typo3 extension:setup && apachectl start"
}