{
    "extends": ["config:recommended"],
    "branchPrefix": "github-renovate/",
    "packageRules": [
    {
        "matchFileNames": ["vendor/**"],
        "groupName": "Composer",
        "versioning": "composer",
        "rangeStrategy": "bump",
        "groupSlug": "composer",
        "postUpgradeTasks": {
        "fileFilters": ["**/composer.json"],
        "executionMode": "update",
        "commands": [
          "composer update 'typo3/cms-*' -W"
        ]
      }
    }
    ]
}
